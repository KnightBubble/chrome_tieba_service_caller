{
	"name": "Service Caller",
	"version": "0.1.1026",
	"description": "call service for any text selected on web page",
	"options_page": "options.html",
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"contextMenus",
		"storage"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"service.js"
		]
	},
	"browser_action": {
		"name": "options",
		"default_icon": "icon.png"
	},
	"manifest_version": 2
}
